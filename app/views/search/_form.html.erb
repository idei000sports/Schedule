<%= form_with(url: search_search_path, method: :get,  local: true) do |form| %>

<div class="form-group">
<%= form.label :start_date , "イベントの開始時間", class:'control-label w-100' %>
		<%= form.date_field :start_date,
			{class:'form-control bootstrap-date',
			required: true}%>
</div>


<div class="form-group">
	<%= form.label :prefecture_id, '都道府県' %>
	<%= form.collection_select :prefecture_id,  @prefecture, :id, :name,{ include_blank: '都道府県を選択'}, { class:'form-control', required: true} %>
</div>


<div class="form_group">
	<%= form.submit "登録", class:'btn' %>
</div>

<% end %>